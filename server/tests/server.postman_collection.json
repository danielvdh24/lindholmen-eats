				{
					"name": "POST /api/restaurants",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Parse the response JSON\r",
									"const jsonData = JSON.parse(responseBody);\r",
									"\r",
									"// Extract the restaurant_id and set it as an environment variable\r",
									"pm.environment.set(\"restaurant_id\", jsonData._id);\r",
									"\r",
									"// Define the expected response data\r",
									"const expectedResponse = {\r",
									"    \"name\": \"Kai's FIRST Restaurant\",\r",
									"    \"address\": \"Palo Alto, CA\"\r",
									"};\r",
									"\r",
									"// Check if the response status code is in the 2xx range (200-299)\r",
									"pm.test(\"Response Status is 200\", () => {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"// Check if the \"name\" and \"address\" fields match the expected values\r",
									"pm.test(\"Name and Address Match Expected Values\", () => {\r",
									"    pm.expect(jsonData.name).to.eql(expectedResponse.name);\r",
									"    pm.expect(jsonData.address).to.eql(expectedResponse.address);\r",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
